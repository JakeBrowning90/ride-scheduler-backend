generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Ride {
    id Int @id @default(autoincrement())
    clientName String
    clientPhone String
    pickUpLocation String
    dropOffLocation String
    pickUpTime DateTime
    passengerCt Int
    hasLuggage Boolean
    jobStatus String
}
